<?xml version="1.0" encoding="utf-8"?>
<configuration>
  
  <configSections>
    <section name="Ideal" type="Ideal.Infrastructure.Configuration.ConfigApplicationSettings, Ideal.Infrastructure, Version=1.0.0.0, Culture=neutral" requirePermission="false">
      
    </section>
  </configSections>
  
  <Ideal>
    <site email="webmaster@wcpro.com" websiteName="Ideal" companyName="WCPRO" />
    <membership 
      multiTenant="false" 
      defaultTenant="ideal" 
      emailIsUsername="false"
      usernamesUniqueAcrossTenants="false"
      requireAccountVerification="true"
      allowLoginAfterAccountCreation="true"
      accountLockoutFailedLoginAttempts="1"
      accountLockoutDuration="2.01:01:01"
      accountLockoutMinutes="1"
      allowAccountDeletion="true"
      minimumPasswordLength="6"
      passwordResetFrequency="5"
      passwordHashingIterationCount="4"
      allowEmailchangeWhenEmailIsUsername="true"
      />
  </Ideal>
</configuration>